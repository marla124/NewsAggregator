@model List<NewModel>
@{
    ViewData["Title"] = "News";
}

<div class="text-center">
    <h2>News</h2>
</div>

<div class="container text-center">
    <div class="row align-items-start">
        @foreach (var item in Model)
        {
            await Html.RenderPartialAsync("NewPreview", item);
        }
    </div>
</div>